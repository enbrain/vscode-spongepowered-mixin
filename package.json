{
  "name": "vscode-spongepowered-mixin",
  "displayName": "Spongepowered Mixin Support",
  "description": "Spongepowered Mixin Support for VSCode (Intellisense)",
  "publisher": "enbrain",
  "repository": {
    "type": "git",
    "url": "https://github.com/enbrain/vscode-spongepowered-mixin.git"
  },
  "icon": "images/icon.png",
  "version": "0.1.1",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "mixin",
    "spongepowered",
    "fabric",
    "forge",
    "bytecode"
  ],
  "engines": {
    "vscode": "^1.71.0"
  },
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "javaExtensions": [
      "./jdtls.ext/io.github.enbrain.jdtls.spongepowered.mixin.plugin/target/io.github.enbrain.jdtls.spongepowered.mixin.plugin-0.1.0.jar"
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "extensionDependencies": [
    "redhat.java"
  ],
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}